{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "env": {
    "NODE_ENV": "development",
    "MONGO_URI": "@mongo_uri",
    "JWT_SECRET": "@jwt_secret",
    "EMAIL_HOST": "@email_host",
    "EMAIL_USER": "@email_user",
    "EMAIL_FROM": "@email_from",
    "EMAIL_PASS": "@email_pass"
  }
}
